{
  "name": "furre-nextjs-portfolio",
  "version": "0.9.0",
  "private": true,
  "scripts": {
    "postinstall": "npx simple-git-hooks",
    "type-check": "tsc --noEmit --skipLibCheck --incremental",
    "dev": "next dev",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "framer-motion": "^9.0.2",
    "next": "^13.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.8.0",
    "react-use-lanyard": "^0.2.0"
  },
  "devDependencies": {
    "@types/node": "^18.16.1",
    "@types/react": "18.2.0",
    "autoprefixer": "^10.4.14",
    "lint-staged": "^13.2.2",
    "openai": "^3.2.1",
    "postcss": "^8.4.23",
    "rome": "^12.0.0",
    "simple-git-hooks": "^2.8.1",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "rome format --write"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged",
    "pre-push": "npm run type-check"
  }
